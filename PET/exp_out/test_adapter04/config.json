{
    "exp_dir": "exp_out/test_adapter04",
    "exp_name": "test_adapter04",
    "allow_skip_exp": true,
    "dataset": "rte",
    "few_shot": true,
    "num_shot": 32,
    "few_shot_random_seed": 100,
    "compute_strategy": "none",
    "num_steps": 300,
    "split_option_at_inference": false,
    "optimizer": "adafactor",
    "lr": 1e-05,
    "trainable_param_names": ".*layer_norm.*|.*adapter.*",
