    "lora_rank": 4,
    "lora_scaling_rank": 0,
    "lora_init_scale": 0.01,
    "lora_modules": "none",
    "lora_layers": "none",
    "bitfit_modules": ".*",
    "bitfit_layers": "q|k|v|o|wi_[01]|w_o",
    "adapter_type": "normal",
    "adapter_non_linearity": "relu",
